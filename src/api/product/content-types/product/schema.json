{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "product"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "sku": {
      "type": "string"
    },
    "oldPrice": {
      "type": "decimal"
    },
    "weightG": {
      "type": "integer"
    },
    "order_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-product.order-product",
      "mappedBy": "product"
    }
  }
}
